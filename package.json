{
  "name": "hapi-path",
  "version": "0.5.1",
  "description": "Quick start your hapi project",
  "main": "index.js",
  "scripts": {
    "test:unit": "lab --leaks -cL",
    "test:acceptance": "cucumber-js --tags=@complete",
    "allTests": "npm run test:unit && npm run test:acceptance",
    "cover:unit": "nyc --silent npm run test:unit",
    "cover:acceptance": "nyc --silent npm run test:acceptance",
    "cover:report": "nyc --reporter=lcov --lines=100 --check-coverage=true --reporter=html npm run allTests"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/momiller121/hapi-path.git"
  },
  "keywords": [
    "hapi",
    "starter"
  ],
  "author": "Mark Miller",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/momiller121/hapi-path/issues"
  },
  "homepage": "https://github.com/momiller121/hapi-path#readme",
  "dependencies": {
    "@hapi/hapi": "^19.1.1",
    "@hapi/hoek": "^9.0.4",
    "@hapi/inert": "^6.0.1",
    "@hapi/joi": "^17.1.1",
    "@hapi/vision": "^6.0.0",
    "hapi-pino": "^6.5.0",
    "hapi-swagger": "^12.1.3",
    "pino": "^5.17.0"
  },
  "devDependencies": {
    "@hapi/code": "^8.0.1",
    "@hapi/eslint-config-hapi": "^13.0.2",
    "@hapi/lab": "^22.0.4",
    "cucumber": "^6.0.5",
    "nyc": "^15.0.0"
  }
}
